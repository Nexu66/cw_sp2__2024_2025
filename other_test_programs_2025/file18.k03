NAME _PROGRAM; 
BODY DATA INTEGER16 _XVALUEE[10], _MAXVALU, _IINDEXT;
  FOR _IINDEXT := 0 TO 9 DO
    GET _XVALUEE[_IINDEXT]
  
  _MAXVALU := _XVALUEE[9]
  PUT _XVALUEE[9]
  FOR _IINDEXT := 8 DOWNTO 0 DO {
    PUT _XVALUEE[_IINDEXT]
    IF _MAXVALU <= _XVALUEE[_IINDEXT] { _MAXVALU := _XVALUEE[_IINDEXT] }
  }
  
  PUT _MAXVALU

END