NAME _PROGRAM; 
BODY DATA INTEGER16 _TOVALUE, _XVALUEE[10], _HVALUEE[10], _RRESULT[19#*10 + 10 - 1*#], _IINDEXT, _JINDEXT, _NORMVAL;
  _TOVALUE := 23
  
  FOR _IINDEXT := 0 TO 9 DO
    _XVALUEE[_IINDEXT] := _TOVALUE
  
  FOR _IINDEXT := 0 TO 9 DO
    _HVALUEE[_IINDEXT] := _TOVALUE

  FOR _IINDEXT := 0 TO 9 DO
    FOR _JINDEXT := 0 TO 9 DO
    _RRESULT[_IINDEXT + _JINDEXT] := _RRESULT[_IINDEXT + _JINDEXT] + _XVALUEE[_IINDEXT] * _HVALUEE[_JINDEXT]
	
  _NORMVAL := _RRESULT[9] DIV _TOVALUE;	
  FOR _IINDEXT := 0 TO 18 DO
    _RRESULT[_IINDEXT] := _RRESULT[_IINDEXT] DIV _NORMVAL;

  FOR _IINDEXT := 0 TO 18 DO
    PUT _RRESULT[_IINDEXT]
END