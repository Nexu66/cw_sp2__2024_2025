NAME _PROGRAM ; 
BODY DATA INTEGER16 _BBBBBBB[10] , _DDDDDDD[10] , _EEEEEEE[10] , _KKKKKKK, _RRESULT[10], _IINDEXT ;
  _KKKKKKK := 25630
  
  FOR _IINDEXT := 0 TO 9 DO {
    _BBBBBBB[_IINDEXT] := _IINDEXT
    _DDDDDDD[_IINDEXT] := _IINDEXT
    _EEEEEEE[_IINDEXT] := _IINDEXT
  }
  
  FOR _IINDEXT := 0 TO 9 DO
    _RRESULT[_IINDEXT] := _KKKKKKK + _BBBBBBB[_IINDEXT] - _DDDDDDD[_IINDEXT] DIV 2 + _EEEEEEE[_IINDEXT] * 4  

  FOR _IINDEXT := 0 TO 9 DO
    PUT _RRESULT[_IINDEXT]
END